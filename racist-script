--configuration
local country = "COUNTRY YOU HATE HERE"
local kickmessage = "TEXT HERE"

--do not touch
local anti = game:GetService("LocalizationService")
game.Players.PlayerAdded:Connect(function(Player)
    local Result, Code = pcall(function()
